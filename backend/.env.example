# Google Calendar API settings
GOOGLE_CALENDAR_CREDENTIALS_PATH=./credentials/google_credentials.json

# Learn platform credentials
LEARN_USERNAME=your_username_here
LEARN_PASSWORD=your_password_here
LEARN_BASE_URL=https://learn.university.com  # Replace with your university's Learn URL

# DeepSeek API settings
