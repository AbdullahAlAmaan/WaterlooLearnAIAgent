DEEPSKEE_API_KEY=your_deepseek_api_key_here

# Logging settings
LOG_LEVEL=INFO  # Set the logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)

# Database settings (if applicable)
DATABASE_URL=sqlite:///./calendar_assistant.db  # Example for SQLite